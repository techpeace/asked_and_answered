.clear{"class"=>"clear"}
  %table{:width => "275px"}
    %tr
      %th
        Recent Questions
    
    - for question in @questions
      %tr
        %td{:id => 'q' + question.id.to_s}
          =link_to h(question.text), question_path(question) 
          -
          %em= byline(question.questioner)